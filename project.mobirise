{
  "settings": {
    "name": "北京市A级旅游景区图鉴",
    "currentPage": "index.html",
    "ai": {
      "prompt": "Please help me generate a website for a map of an A-level tourist attraction in Beijing",
      "imageSearchQuery": "landmarks"
    },
    "theme": {
      "name": "startm5",
      "title": "StartM5",
      "styling": {
        "backgroundColor": "#F6E7ED",
        "primaryColor": "#F55D3E",
        "secondaryColor": "#ECC84D",
        "successColor": "#29D786",
        "infoColor": "#0ECCF2",
        "warningColor": "#FFC001",
        "dangerColor": "#D92738",
        "mainFont": "Roboto Flex",
        "display1Font": "Roboto Flex",
        "display1Size": 5,
        "display2Font": "Roboto Flex",
        "display2Size": 4,
        "display5Font": "Roboto Flex",
        "display5Size": 2.5,
        "display7Font": "Roboto Flex",
        "display7Size": 1.4,
        "display4Font": "Roboto Flex",
